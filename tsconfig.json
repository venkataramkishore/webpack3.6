{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6", // Needed es6 to resolve the jquery iterable issue
        //https://github.com/DefinitelyTyped/DefinitelyTyped/issues/17239
        "sourceMap": true,
        "outDir": "./src/",
        "noImplicitAny": true
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src/*"
    ]
}